<UserControl x:Class="NetKeyServerGUI.InputConfigList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NetKeyServerGUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="keyLabel" Content="Key #" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Rectangle x:Name="pressedIndicator" HorizontalAlignment="Left" Height="26" Margin="54,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="142" Fill="White"/>
        <Button x:Name="assignmentButton" Content="Button" HorizontalAlignment="Left" Margin="116,41,0,0" VerticalAlignment="Top" Width="80" Height="26" Click="assignmentButton_Click"/>
        <Label Content="Assign" HorizontalAlignment="Left" Margin="66,41,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
