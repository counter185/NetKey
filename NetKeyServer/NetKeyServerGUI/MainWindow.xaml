<Window x:Class="NetKeyServerGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetKeyServerGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="287" Width="410">
    <Grid>
        <ListBox x:Name="devicesList" Margin="10,69,10,10"/>
        <Label Content="Device list" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Double click to edit configuration..." HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ipconfig" Content="Show IP Configuration..." Margin="0,8,10,0" VerticalAlignment="Top" Click="ipconfig_Click" HorizontalAlignment="Right" Width="130"/>

    </Grid>
</Window>
