<Window x:Class="NetKeyServerGUI.DevicePropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetKeyServerGUI"
        mc:Ignorable="d"
        Title="DevicePropertiesWindow" Height="450" Width="951">
    <Grid>
        <ListBox x:Name="deviceInputBindings" Margin="286,41,10,10"/>
        <Label Content="Inputs" HorizontalAlignment="Left" Margin="286,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Device name:" HorizontalAlignment="Left" Margin="10,126,0,0" VerticalAlignment="Top"/>
        <Label Content="Device properties" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Label x:Name="idtext" Content="ID:" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Width="271"/>
        <TextBox HorizontalAlignment="Left" Margin="151,130,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130"/>
        <Label x:Name="pingtext" Content="Latency:" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" Width="271"/>

    </Grid>
</Window>
